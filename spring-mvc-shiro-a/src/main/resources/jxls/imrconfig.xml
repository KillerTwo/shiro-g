<?xml version="1.0" encoding="UTF-8"?> 
<workbook> 
   <worksheet name="Sheet1"> 
    <section startRow="0" endRow="0"/> 
    <loop startRow="1" endRow="1" items="employees" var="employee" varType="org.lwt.entity.Animal">
	    <section startRow="1" endRow="1"> 
	    <mapping row="1" col="0">employee.name</mapping> 
	    <mapping row="1" col="1">employee.species</mapping> 
	    <mapping row="1" col="2">employee.num</mapping> 
	   </section> 
	    <loopbreakcondition> 
	     <rowcheck offset="0">
	        <cellcheck offset="0"></cellcheck> 
	      </rowcheck> 
	    </loopbreakcondition> 
    </loop>
   </worksheet> 
</workbook>